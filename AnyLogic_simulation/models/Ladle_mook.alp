								<Code><![CDATA[// --- On at exit action for EAF blocks (FINAL CLEAN VERSION) ---

Ladle currentLadle = (Ladle) agent;
String pythonPath = "D:/anaconda/python.exe";
String scriptPath = "D:/钢铁电力负荷预测/python/run_eaf_fmu.py";
String command = "\"" + pythonPath + "\" \"" + scriptPath + "\" " + 
                 (currentLadle.mass * 1000) + " " + 
                 targetTemp_EAF_C + " " +
                 currentLadle.temperature;

traceln("Executing EAF command for Ladle " + currentLadle.getId() + ": " + command);

try {
    Process process = Runtime.getRuntime().exec(command);
    StringBuilder output = new StringBuilder();
    StringBuilder errorOutput = new StringBuilder();

    Thread outputThread = new Thread(new StreamGobbler(process.getInputStream(), s -> output.append(s)));
    Thread errorThread = new Thread(new StreamGobbler(process.getErrorStream(), s -> errorOutput.append(s).append("\n")));
    
    outputThread.start();
    errorThread.start();

    int exitCode = process.waitFor();
    outputThread.join(); 
    errorThread.join();

    if (exitCode == 0) {
        if (errorOutput.length() > 0) {
            traceln("DEBUG info from Python script (EAF):\n" + errorOutput.toString());
        }
        String result = output.toString().trim();
        if (!result.isEmpty()) {
            traceln("Result received from EAF_FMU: " + result);
            String[] values = result.split(",");
            if (values.length == 2) {
                double finalTemperature = Double.parseDouble(values[0]);
                double finalMass = Double.parseDouble(values[1]) / 1000;
                currentLadle.temperature = finalTemperature;
                currentLadle.mass = finalMass;
                traceln("Ladle " + currentLadle.getId() + " updated by EAF: Temp=" + finalTemperature + " °C, Mass=" + finalMass + " t");
            } else {
                traceln("ERROR: EAF_FMU script output format is incorrect. Expected 'temp,mass', but got: '" + result + "'");
            }
        } else {
            traceln("Warning: Python script (EAF) finished but produced no standard output.");
        }
    } else {
        traceln("ERROR: Python script (EAF) exited with a non-zero code: " + exitCode);
        if (errorOutput.length() > 0) {
            traceln("ERROR details from Python script (EAF):\n" + errorOutput.toString());
        }
    }
} catch (Exception e) {
    e.printStackTrace();
}]]></Code>
