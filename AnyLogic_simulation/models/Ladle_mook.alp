								<Code><![CDATA[// --- On enter action for CC (FIXED VERSION) ---

Ladle currentLadle = (Ladle) agent;

String pythonPath = "D:/anaconda/python.exe"; 
String scriptPath = "D:/钢铁电力负荷预测/python/run_cc_fmu.py"; 

String command = "\"" + pythonPath + "\" \"" + scriptPath + "\" " + 
                 currentLadle.temperature + " " + 
                 (currentLadle.mass * 1000) + " " + 
                 castingSpeed;

traceln("CC Logic Start for Ladle " + currentLadle.getId());

try {
    Process process = Runtime.getRuntime().exec(command);
    StringBuilder output = new StringBuilder();
    StringBuilder errorOutput = new StringBuilder();

    Thread outputThread = new Thread(new StreamGobbler(process.getInputStream(), s -> output.append(s).append("\n")));
    Thread errorThread = new Thread(new StreamGobbler(process.getErrorStream(), s -> errorOutput.append(s).append("\n")));
    
    outputThread.start();
    errorThread.start();

    int exitCode = process.waitFor();
    outputThread.join(); 
    errorThread.join();

    if (exitCode == 0) {
        String fullOutput = output.toString();

        String[] lines = fullOutput.split("\\r?\\n"); 
        boolean success = false; 

        for (String line : lines) {
            line = line.trim();

            if (line.isEmpty() || !line.contains(",")) {
                continue; 
            }

            try {
                String[] values = line.split(",");
                if (values.length >= 2) {
                    double finalTemp = Double.parseDouble(values[0]);
                    double durationSec = Double.parseDouble(values[1]);

                    currentLadle.temperature = finalTemp;
                    currentLadle.cc_casting_time = durationSec;
                    
                    traceln("CC Data Found: Time=" + durationSec + "s, OutTemp=" + finalTemp);
                    success = true;
                    break; 
                }
            } catch (NumberFormatException e) {
                continue;
            }
        }

        if (!success) {
            traceln("WARNING: CC Python finished but no valid data line found. Output was:\n" + fullOutput);
            currentLadle.cc_casting_time = 3600;
        }

    } else {
        traceln("ERROR: CC Script ExitCode=" + exitCode);
        currentLadle.cc_casting_time = 3600; 
    }

} catch (Exception e) {
    e.printStackTrace();
    currentLadle.cc_casting_time = 3600; 
}]]></Code>
