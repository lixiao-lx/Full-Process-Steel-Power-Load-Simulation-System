							<Value Class="CodeValue">
								<Code><![CDATA[// --- On at exit action for delayLF block (FINAL CLEAN VERSION) ---


Ladle currentLadle = (Ladle) agent;

String pythonPath = "D:/anaconda/python.exe"; 
String scriptPath = "D:/钢铁电力负荷预测/python/run_lf_fmu.py";
String command = "\"" + pythonPath + "\" \"" + scriptPath + "\" " + 
                 (currentLadle.mass * 1000) + " " +  
                 currentLadle.temperature + " " +   
                 targetTemp_LF_C;                   

traceln("Executing LF command for Ladle " + currentLadle.getId() + ": " + command);

try {
    Process process = Runtime.getRuntime().exec(command);

    StringBuilder output = new StringBuilder();
    StringBuilder errorOutput = new StringBuilder();

    Thread outputThread = new Thread(new StreamGobbler(process.getInputStream(), s -> output.append(s)));
    Thread errorThread = new Thread(new StreamGobbler(process.getErrorStream(), s -> errorOutput.append(s).append("\n")));
    
    outputThread.start();
    errorThread.start();

    int exitCode = process.waitFor();
    outputThread.join(); 
    errorThread.join();

    if (exitCode == 0) { // 进程正常退出
        if (errorOutput.length() > 0) {
            traceln("DEBUG info from Python script (LF):\n" + errorOutput.toString());
        }

        String result = output.toString().trim();
        if (!result.isEmpty()) {
            traceln("Result received from LF_FMU: " + result);

            String[] values = result.split(",");
            
            if (values.length == 2) {

                double finalTemperatureLF = Double.parseDouble(values[0]);
                double finalMassLF = Double.parseDouble(values[1]) / 1000; 
                currentLadle.temperature = finalTemperatureLF;
                currentLadle.mass = finalMassLF;
                
                traceln("Ladle " + currentLadle.getId() + " updated by LF: Temp=" + finalTemperatureLF + " °C, Mass=" + finalMassLF + " t");
            } else {
                traceln("ERROR: LF_FMU script output format is incorrect. Expected 'temp,mass', but got: '" + result + "'");
            }
            
        } else {
            traceln("Warning: LF_FMU script finished successfully but produced no standard output.");
        }
    } else { 
        traceln("ERROR: LF_FMU script exited with non-zero code: " + exitCode);
        if (errorOutput.length() > 0) {
            traceln("ERROR details from LF script:\n" + errorOutput.toString());
        }
    }

} catch (Exception e) {
    e.printStackTrace();
}]]></Code>
